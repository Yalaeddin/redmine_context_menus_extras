<% if @redirect_to %>
  window.location.replace("<%= @redirect_to %>");
<% else %>
  $('#ajax-modal').dialog('destroy');
  $('#relation_issue_to_id').autocomplete('destroy');

  $('#ajax-modal').html('<%= escape_javascript(render :partial => 'issue_relations/bulk_new') %>');
  showModal('ajax-modal', '500px');
  $('#ajax-modal').addClass('bulk-new-relations');
  $('ul.ui-autocomplete').css('z-index', '1500');

  $('#ajax-modal').on('dialogdragstop', function() {
    $('ul.ui-autocomplete').css('z-index', parseInt($('#ajax-modal').closest('.ui-dialog').css('z-index')) + 1);
  });
<% end %>
